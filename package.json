{
    "name": "jetcake-project",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "client": "cd client && npm run start",
        "start": "node ./bin/www",
        "server": "nodemon ./bin/www",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "helmet": "^3.21.2",
        "jimp": "^0.9.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.8.10",
        "morgan": "~1.9.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.0.1"
    },
    "devDependencies": {
        "concurrently": "^5.1.0",
        "nodemon": "^2.0.2"
    }
}
